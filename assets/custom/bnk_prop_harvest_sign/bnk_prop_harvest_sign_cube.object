<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="custom/bnk_prop_harvest_sign/bnk_prop_harvest_sign" orientation_object="rp_bnk_prop_harvest_sign_cube" />
	<sequence_manager file="custom/bnk_prop_harvest_sign/bnk_prop_harvest_sign_cube"/>
	
	<bodies>
		<body name="animated_body" enabled="true" template="animated">
			<object name="c_box_01" collision_type="box" padding="-2.5"/>
			<object name="g_bnk_prop_harvest_sign_cube" />
		</body>
		<body name="static_body" enabled="true" template="animated">
			<object name="rp_bnk_prop_harvest_sign_cube" />
			<object name="c_capsule_01" collision_type="capsule" padding="-2.5"/>
		</body>
	</bodies>	
  
	<animations>
		<animation_group name="cube_loop" enabled="true" loop="true">
			<object name="g_bnk_prop_harvest_sign_cube"/>
		</animation_group>
	</animations>
  
	<decal_surfaces default_material="metal">
		<decal_mesh name="g_bnk_prop_harvest_sign_cube" enabled="true" material="sheet_metal"/>
		<decal_mesh name="g_bnk_prop_harvest_sign_cube_pole" enabled="true" material="sheet_metal"/>
	</decal_surfaces>
	
	<graphics>
		<object name="g_bnk_prop_harvest_sign_cube" enabled="true"  shadow_caster="true"/>
		<object name="g_bnk_prop_harvest_sign_cube_pole" enabled="true"  shadow_caster="true"/>
	</graphics>
</dynamic_object> 