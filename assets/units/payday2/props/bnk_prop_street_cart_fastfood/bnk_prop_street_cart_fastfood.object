			<object name="c_box_cart_10" collision_type="box" padding="-2.5"/>
			<object name="c_box_cart_11" collision_type="box" padding="-2.5"/>
			<object name="c_box_cart_12" collision_type="box" padding="-2.5"/>			
        </body>
		
        <body name="static_player_blocker" template="bag" keyframed="true" enabled="true" collision_class="bag_blocker">
			<object name="a_hinge_cart" />
			<object name="c_box_bag" collision_type="box" padding="-2.5"/>
        </body>
		
        <body name="static_objects" template="static" enabled="true" >
			<object name="rp_bnk_prop_street_cart_fastfood" />
			<object name="c_box_shelf_01" collision_type="box" padding="-2.5"/>
			<object name="c_box_shelf_02" collision_type="box" padding="-2.5"/>
			<object name="c_box_shelf_03" collision_type="box" padding="-2.5"/>
			
			<object name="c_capsule_container_01" 	collision_type="capsule" 	padding="-2.5"/>
			<object name="c_box_container_02" 		collision_type="box" 		padding="-2.5"/>
			<object name="c_box_container_03" 		collision_type="box" 		padding="-2.5"/>
        </body>
		
        <body name="static_damaged" template="static" enabled="false" >
        	<object name="rp_bnk_prop_street_cart_fastfood" />
			<object name="c_box_support_dmg_01" collision_type="box" padding="-2.5"/>
			<object name="c_convex_support_dmg_01" collision_type="convex" padding="-2.5"/>
        </body>
		
        <body name="debri_wheel_01" template="debris"  enabled="false" >
			<object name="c_convex_wheel_01" collision_type="convex" padding="-2.5"/>
        </body>
        <body name="debri_wheel_02" template="debris"  enabled="false" >
			<object name="c_convex_wheel_02" collision_type="convex" padding="-2.5"/>
        </body>
        <body name="dynamic_trashcan" template="debris"  enabled="false" >
			<object name="c_convex_trashcan" collision_type="convex" 	material="plastic" padding="-2.5"/>
        </body>
        <body name="dynamic_sign" template="debris"  enabled="false" >
			<object name="c_convex_sign" collision_type="convex"		material="plastic" padding="-2.5"/>
        </body>
		
        <body name="dynamic_container_01" template="debris"  enabled="false" >
			<object name="c_capsule_container_01" collision_type="capsule" material="plastic" padding="-2.5"/>
        </body>
        <body name="dynamic_container_02" template="debris"  enabled="false" >
			<object name="c_box_container_02" collision_type="box" material="plastic" padding="-2.5"/>
        </body>
        <body name="dynamic_container_03" template="debris"  enabled="false" >
			<object name="c_box_container_03" collision_type="box" material="plastic" padding="-2.5"/>
        </body>
		
		
        <body name="dynamic_shelf_01" template="debris"  enabled="false" remove_ray_type="bullet">
			<object name="c_box_shelf_01" collision_type="box" padding="-2.5"/>
			<object name="g_dyn_shelf_01" />
        </body>	
        <body name="dynamic_shelf_02" template="debris"  enabled="false">
			<object name="c_box_shelf_02" collision_type="box" padding="-2.5"/>
			<object name="g_dyn_shelf_02" />
        </body>	
        <body name="dynamic_shelf_03" template="debris"  enabled="false">
			<object name="c_box_shelf_03" collision_type="box" padding="-2.5"/>
			<object name="g_dyn_shelf_03" />
        </body>
		
