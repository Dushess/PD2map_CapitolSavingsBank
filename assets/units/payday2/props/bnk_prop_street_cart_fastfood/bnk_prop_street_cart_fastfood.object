    <graphics>
		<!-- Support Parts -->
        <object name="g_dest_remove" 		enabled="true"	/>
        <object name="g_cart"		 		enabled="true"	/>
		<object name="g_support" 			enabled="true"	/>
		<object name="g_support_dmg" 		enabled="false" />
		<object name="g_debris_wheel_01" 	enabled="true"  />
